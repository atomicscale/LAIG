<SCENE>
 
<INITIALS>
    <frustum near="0.1" far="250"/>                
    <translation x="-1" y="1" z="0" />              
    <rotation axis="x" angle="0" />                
    <rotation axis="y" angle="0" />                
    <rotation axis="z" angle="0" />                
    <scale sx="0.25" sy="0.25" sz="0.25" />        
    <reference length="5" />                        
</INITIALS>
 
<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />                                  
    <background r="0" g="0" b="0" a="1" />        
</ILLUMINATION>
 
<LIGHTS>
    <LIGHT id="LightA">                                    
        <enable value="true" />                        
        <position x="8.0" y="-4" z="8.0" w="1.0" />      
        <ambient r="0.4" g="0.4" b="0.4" a="1.0" />        
        <diffuse r="0.4" g="0.4" b="0.54" a="1.0" />        
        <specular r="0.7" g="0.7" b="0.7" a="1.0" />      
    </LIGHT>
 
    <LIGHT id="LightB">                                    
        <enable value="true" />                        
        <position x="4.0" y="-4" z="5.5" w="0.0" />      
        <ambient r="0.3" g="0.3" b="0.3" a="1.0" />        
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />        
        <specular r="0.9" g="0.9" b="0.9" a="0.9" />      
    </LIGHT>
 
    <LIGHT id="LightC">                                    
        <enable value="true" />                        
        <position x="4.5" y="-4" z="11.0" w="0.0" />      
        <ambient r="0.4" g="0.4" b="0.4" a="1.0" />        
        <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />        
        <specular r="0.9" g="0.9" b="0.9" a="1.0" />    
    </LIGHT>
</LIGHTS>
 
<TEXTURES>
    <!--Engine-->
    <TEXTURE id="leather">
        <file path="textures/leather.jpg" />                          
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="fuselagem">
        <file path="textures/fuselagem.jpg" />                          
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="sky">
        <file path="textures/sky.jpg" />                          
        <amplif_factor s="1" t="1" />
    </TEXTURE>
 
    <!--Floor-->
    <TEXTURE id="road">
        <file path="textures/estrada.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>
</TEXTURES>
 
<MATERIALS>
     <MATERIAL id="defaultMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1.0" />        
        <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />      
        <ambient r="0.5" g="0.5" b="0.5" a="1.0" />        
        <emission r="0.0" g="0.0" b="0.0" a="1.0" />        
    </MATERIAL>

    <MATERIAL id="black">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1.0" />        
        <diffuse r="0.0" g="0.0" b="0.0" a="1.0" />      
        <ambient r="0.0" g="0.0" b="0.0" a="1.0" />        
        <emission r="0.0" g="0.0" b="0.0" a="1.0" />        
    </MATERIAL>

</MATERIALS>
 
<LEAVES>
    <LEAF id="rectangulo" type="rectangle" args="0 4 5 0" />
    <LEAF id="cilindro" type="cylinder" args="4 1 1 20 20" />
    <LEAF id="esfera" type="sphere" args="2 20 20" />
    <LEAF id="triangulo" type="triangle" args="2 0 12 12 0 2 0 0 0" />
    <LEAF id="asa" type="cylinder" args="0.7 0.2 0.1 20 20" />
    <LEAF id="front" type="cylinder" args="0.7 0.01 0.1 20 20" />
    <LEAF id="vehicle" type="vehicle" />
    <LEAF id="terrain" type="terrain" texture="ii" heightmap="ii" />
    <LEAF id="plane" type="plane" parts="ii" />
    <LEAF id="patch" type="patch" order="ii" partsU="ii" partsV="ii">
    	<controlpoint x="ff" y="ff" z="ff" />
    	<controlpoint x="ff" y="ff" z="ff" />
    	<controlpoint x="ff" y="ff" z="ff" />

    	<controlpoint x="ff" y="ff" z="ff" />
    	<controlpoint x="ff" y="ff" z="ff" />
    	<controlpoint x="ff" y="ff" z="ff" />

    	<controlpoint x="ff" y="ff" z="ff" />
    	<controlpoint x="ff" y="ff" z="ff" />
    	<controlpoint x="ff" y="ff" z="ff" />

    </LEAF>
</LEAVES>
 
 <animations>
        <animation id="preparetolift" span="4" type="linear">
            <controlpoint xx="0" yy="-2" zz="0" />
            <controlpoint xx="0" yy="4" zz="0" />
        </animation>

        <animation id="rotation" span="2" type="circular" center="0 4 0" radius="0" startang="-90" rotang="180" />

        <animation id="liftoff" span="5" type="linear">
            <controlpoint xx="0" yy="4" zz="0" />
            <controlpoint xx="0" yy="5" zz="-1" />
            <controlpoint xx="14" yy="14" zz="-14" />
        </animation>

        <animation id="teste" span="1" type="linear">
            <controlpoint xx="0" yy="0" zz="0" />
            <controlpoint xx="3" yy="1" zz="0" />
            <controlpoint xx="0" yy="0" zz="0" />
        </animation>

        <animation id="teste2" span="5" type="linear">
            <controlpoint xx="0" yy="0" zz="0" />
            <controlpoint xx="-3" yy="-1" zz="0" />
            <controlpoint xx="0" yy="0" zz="0" />
        </animation>

        <animation id="circ" span="2" type="circular" center="0 0 0" radius="0" startang="0" rotang="360" />

    </animations>

<NODES>
 
    <ROOT id="main" />    
 
    <NODE id="main">      
        <MATERIAL id="defaultMaterial" />      
        <TEXTURE id="null" />                
 
        <DESCENDANTS>
          //  <DESCENDANT id="estrada" />
            <DESCENDANT id="aviao" />
           // <DESCENDANT id="paisagem"/>
        </DESCENDANTS>
    </NODE>
   
