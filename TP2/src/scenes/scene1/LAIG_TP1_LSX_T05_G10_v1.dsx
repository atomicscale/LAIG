        <rotate axis="x" angle="-90" />
 
        <children>
            <primitiveref id="rectangulo" />              
        </children>
    </component>
 
    <component id="aviao">      
        <material id="null" />    
        <texture id="null" />

        <rotate axis="x" angle="-90" />

 		 <animationref id="flying" />
         <animationref id="rotation1" />
         <rotate axis="y" angle="90" />
         <animationref id="fly" />
         <animationref id="rotation2" />
         <rotate axis="y" angle="-90" />
         <animationref id="flying" />
         <animationref id="rotation1" />
         <rotate axis="y" angle="90" />
         <animationref id="fly" />
         <animationref id="rotation2" />
         <rotate axis="y" angle="-90" />

        <children>
