    <NODE id="aviao">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <animationref id="preparetolift" />
        <animationref id="rotation" />
        <animationref id="liftoff" />
 
        <DESCENDANTS>
            <DESCENDANT id="parteSup" />
            <DESCENDANT id="parteInf" />  
            <DESCENDANT id="vehicle" />                  
        </DESCENDANTS>
    </NODE>

   
 
