<dsx>
 


<views default="p1" >

        <perspective id="p1" near="0.1" far="500" angle="22">
            <from x="-70" y="40" z="10" />
            <to x="30" y="10" z="10" />
        </perspective>

        <perspective id="p2" near="0.1" far="1500" angle="40">
            <from x="-5" y="5" z="60" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="p3" near="0.1" far="1500" angle="90">
            <from x="100" y="25" z="200" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="p4" near="0.1" far="1500" angle="50">
            <from x="300" y="0" z="800" />
            <to x="3" y="0" z="0" />
        </perspective>
    </views>
 
<illumination doublesided="1" local="1">
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />                                  
    <background r="0.203" g="0.8" b="1" a="1" />        
</illumination>
 
<lights>

        <!-- Deve existir um ou mais blocos "omni" ou "spot" -->
        <!-- Os identificadores "id" nao podem ser repetidos -->
        <omni id="omni1" enabled="1" >
            <location x="26" y="26" z="56" w="0.2" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </omni>

        <omni id="omni2" enabled="1" >
            <location x="0" y="100" z="100" w="0.2" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </omni>

        <omni id="omni3" enabled="1" >
            <location x="-26" y="26" z="56" w="0.2" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </omni>

        <spot id="spot1" enabled="1" angle="40" exponent="0.5">
            <!-- atencao, "target" e' diferente de "direction" -->
            <target x="3" y="3" z="0.5" />
            <location x="4" y="2" z="2" />
            <ambient r="0.5" g="0.5" b="0.5" a="0.5" />
            <diffuse r="0.5" g="0.5" b="0.5" a="0.5" />
            <specular r="0.5" g="0.5" b="0.5" a="0.5" />
        </spot>
    </lights>
 
<textures>
    <!--Engine-->
        <texture id="leather" file="textures/leather.jpg" length_s="1.0" length_t="1.0" />
        <texture id="fuselagem" file="textures/fuselagem.jpg" length_s="1.0" length_t="1.0" />
        <texture id="road" file="textures/estradaz.jpg" length_s="5.0" length_t="4.0" />
        <texture id="landing" file="textures/roadz.jpg" length_s="5.0" length_t="4.0" />
        <texture id="boardTex" file="textures/ground.jpg" length_s="1.0" length_t="1.0" />
        <texture id="marsTex" file="textures/marble.png" length_s="1.0" length_t="1.0" />
</textures>

    <materials>

        <!-- Deve existir um ou mais blocos "material" -->
        <!-- Os identificadores "id" nao podem ser repetidos -->
        <material id="defaultmaterial" >
            <shininess value="1" />
            <specular r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <ambient r="0.5" g="0.5" b="0.5" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />     
        </material>

        <material id="black" >
            <shininess value="0.8" />
            <specular r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>

    </materials>

<primitives>

    <primitive id="rectangulo" type="rectangle" args="0 4 5 0" />
    <primitive id="cilindro" type="cylinder" args="4 1 1 20 20" />
    <primitive id="esfera" type="sphere" args="2 20 20" />
    <primitive id="triangulo" type="triangle" args="2 0 12 12 0 2 0 0 0" />
    <primitive id="asa" type="cylinder" args="0.7 0.2 0.1 20 20" />
    <primitive id="front" type="cylinder" args="0.7 0.01 0.1 20 20" />
    <primitive id="toro" type="torus" args="0.75 0.5 100 100" />
    <primitive id="plano" type="plane" dimX="5" dimY="5" partsX="20" partsY="20"/>
    <primitive id="patch" type="patch" orderU="2" orderV="3" partsU="20" partsV="20">
                <controlpoint x="-2.0" y="-2.0" z="1.0" />
               <controlpoint x="-2.0" y="-1.0" z="-2.0" />
               <controlpoint x="-2.0" y="1.0" z="5.0" />
               <controlpoint x="-2.0" y="2.0" z="-1.0" />

               <controlpoint x="0" y="-2.0" z="0" />
               <controlpoint x="0" y="-1.0" z="-1.0" />
               <controlpoint x="0" y="1.0" z="1.5" />
               <controlpoint x="0" y="2.0" z="0" />

               <controlpoint x="2.0" y="-2.0" z="-1.0" />
               <controlpoint x="2.0" y="-1.0" z="2.0" />
               <controlpoint x="2.0" y="1.0" z="-5.0" />
               <controlpoint x="2.0" y="2.0" z="1.0" />
    </primitive>
    <primitive id ="xadrez" type="chessboard" du="10" dv="10" textureref="textures/marble.png" su="2" sv="1">
                   <c1 r="0" g="0.4" b="0.2" a="1.0" />
                   <c2 r="0.7" g="0.6" b="0.0" a="1.0" />
                   <cs r="1.0" g="0.0" b="1.0" a="1.0" />
    </primitive>

</primitives>

<animations>
        <animation id="flying" span="4" type="linear">
            <controlpoint xx="0" yy="50" zz="0" />
            <controlpoint xx="150" yy="50" zz="0" />
        </animation>

        <animation id="rotation1" span="5" type="circular" center="150 50 -50" radius="50" startang="0" rotang="180" />

        <animation id="fly" span="4" type="linear">
            <controlpoint xx="150" yy="50" zz="-100" />
            <controlpoint xx="0" yy="50" zz="-100" />
        </animation>

        <animation id="rotation2" span="5" type="circular" center="0 50 -50" radius="50" startang="180" rotang="180" />

</animations>
 
<components>
 
    <root id="main" />    
 
    <component id="main">      
        <material id="defaultmaterial" />      
        <texture id="null" />                
 
        <children>

                <!-- deve existir uma ou mais tags "componentref" e/ou -->
                <!-- "componentref", identificando outros -->
                <!-- componentes ou primitivas -->
                <componentref id="chessboard1" />
                <componentref id="planos" />
                <componentref id="patches" />
                <componentref id="estrada" />              
                <componentref id="landing" />
                <componentref id="aviao" />
                

        </children>
    </component>

    <component id="chessboard1">

           <material id="null"  />
           <texture id="null" />
        

            <translate x="-20" y="0" z="0" />
            

            <children>
                <primitiveref id="xadrez" />
            </children>

    </component>

    <component id="planos">
            <material id="null"  />
            <texture id="marsTex" />

            <translate x="0" y="0" z="0" />

            <children>
                <primitiveref id="plano" />
            </children>

        </component>

    <component id="patches">
            <material id="null"  />
            <texture id="boardTex" />

            <translate x="-10" y="0" z="0" />


            <children>
                <primitiveref id="patch" />
            </children>

        </component>
   
    <component id="estrada">      
        <material id="null" />    
        <texture id="road" />                  
 
        <scale sx="500" sy="1" sz="100" />
        <translate x="-4" y="-4" z="2.5" />
        <rotate axis="x" angle="-90" />
    
        <children>
