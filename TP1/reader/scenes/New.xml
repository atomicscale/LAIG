<SCENE>

<INITIALS>
    <frustum near="0.1" far="250"/>                 
    <translation x="-1" y="1" z="0" />               
    <rotation axis="x" angle="0" />                
    <rotation axis="y" angle="0" />                
    <rotation axis="z" angle="0" />                
    <scale sx="0.25" sy="0.25" sz="0.25" />         
    <reference length="5" />                        
</INITIALS>

<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />                                  
    <background r="0.2" g="0.2" b="0.2" a="0.2" />         
</ILLUMINATION>

<LIGHTS>
    <LIGHT id="LightA">                                    
        <enable value="true" />                         
        <position x="8.0" y="8.0" z="8.0" w="1.0" />       
        <ambient r="0.4" g="0.4" b="0.4" a="1.0" />        
        <diffuse r="0.4" g="0.4" b="0.54" a="1.0" />        
        <specular r="0.7" g="0.7" b="0.7" a="1.0" />       
    </LIGHT>

    <LIGHT id="LightB">                                    
        <enable value="true" />                         
        <position x="4.0" y="8.0" z="5.5" w="0.0" />       
        <ambient r="0.3" g="0.3" b="0.3" a="1.0" />        
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />        
        <specular r="0.9" g="0.9" b="0.9" a="0.9" />       
    </LIGHT>

    <LIGHT id="LightC">                                    
        <enable value="true" />                         
        <position x="4.5" y="8.0" z="11.0" w="0.0" />       
        <ambient r="0.4" g="0.4" b="0.4" a="1.0" />        
        <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />        
        <specular r="0.9" g="0.9" b="0.9" a="1.0" />    
    </LIGHT> 
</LIGHTS>

<TEXTURES>
    <!--Wood-->
    <TEXTURE id="Wood">
        <file path="scenes/images/wood.jpg" />                           
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!--RightWall-->
    <TEXTURE id="RightWall">
        <file path="scenes/images/rightwall.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!--LeftWall-->
    <TEXTURE id="LeftWall">
        <file path="scenes/images/leftwall.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!--Floor-->
    <TEXTURE id="Floor">
        <file path="scenes/images/floor.png" />
        <amplif_factor s="0.25" t="0.5" />
    </TEXTURE> 
</TEXTURES>

<MATERIALS>
     <MATERIAL id="defaultMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1.0" />        
        <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />       
        <ambient r="0.0" g="0.0" b="0.0" a="1.0" />         
        <emission r="0.0" g="0.0" b="0.0" a="1.0" />        
    </MATERIAL>
</MATERIALS>

<LEAVES>
    <LEAF id="rectangulo" type="rectangle" args="0 4 5 0" />
    <LEAF id="cilindro" type="cylinder" args="4 1 1 20 20" /> 
    <LEAF id="esfera" type="sphere" args="2 20 20" />
    <LEAF id="triangulo" type="triangle" args="2 0 12 12 0 2 0 0 0" />
</LEAVES>

<NODES>

    <ROOT id="root" />     

    <NODE id="root">       
        <MATERIAL id="defaultMaterial" />       
        <TEXTURE id="null" />                 

        <DESCENDANTS>
            <DESCENDANT id="mesa" />
            <DESCENDANT id="paredeEsquerda" /> 
            <DESCENDANT id="paredeDireita" />   
            <DESCENDANT id="chao" />
            <DESCENDANT id="candeeiro" />      
        </DESCENDANTS>
    </NODE>
    
    <NODE id="chao">      
        <MATERIAL id="null" />    
        <TEXTURE id="Floor" />                  

        <SCALE sx="6" sy="1" sz="5" />
        <TRANSLATION x="0" y="0" z="4" />
        <ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />              
        </DESCENDANTS>
    </NODE>

    <NODE id="paredeEsquerda">       
        <MATERIAL id="null" />    
        <TEXTURE id="LeftWall" />

        <SCALE sx="6" sy="1" sz="5" />
        <TRANSLATION x="0" y="0" z="4" />
        <ROTATION axis="z" angle="-270" />

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />                     
        </DESCENDANTS>
    </NODE>

    <NODE id="paredeDireita">       
        <MATERIAL id="null" />    
        <TEXTURE id="RightWall" />

        <SCALE sx="6" sy="1" sz="5" />
        <TRANSLATION x="0" y="0" z="4" />

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />                     
        </DESCENDANTS>
    </NODE>

    <NODE id="candeeiro">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <TRANSLATION x="4.5" y="8.0" z="7.5" /> 

        <DESCENDANTS>
            <DESCENDANT id="bola" />
            <DESCENDANT id="fio" />                     
        </DESCENDANTS>
    </NODE>

    <NODE id="bola">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="esfera" />                    
        </DESCENDANTS>
    </NODE>

    <NODE id="fio">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <!--Translate-->
        <!--Scale-->

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />                    
        </DESCENDANTS>
    </NODE>

    <NODE id="mesa">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <TRANSLATION x="3.5" y="0.0" z="4.5" />

        <DESCENDANTS>
            <DESCENDANT id="tampo" />
            <DESCENDANT id="pernas" />
        </DESCENDANTS>
    </NODE>

    <NODE id="tampo">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <DESCENDANTS>
            <DESCENDANT id="tampo_de_cima" />
            <DESCENDANT id="tampo_esqerdo" />
            <DESCENDANT id="tampo_direito" />
            <DESCENDANT id="tampo_frente" />
            <DESCENDANT id="tampo_esqerdo" />
            <DESCENDANT id="tampo_de_baixo" />
        </DESCENDANTS>
    </NODE>

    <NODE id="tampo_de_cima">       
        <MATERIAL id="null" />    
        <TEXTURE id="Wood" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />
        </DESCENDANTS>
    </NODE>

    <NODE id="tampo_esqerdo">       
        <MATERIAL id="null" />    
        <TEXTURE id="Wood" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />
        </DESCENDANTS>
    </NODE>

    <NODE id="tampo_direito">       
        <MATERIAL id="null" />    
        <TEXTURE id="Wood" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />
        </DESCENDANTS>
    </NODE>

    <NODE id="tampo_frente">       
        <MATERIAL id="null" />    
        <TEXTURE id="Wood" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />
        </DESCENDANTS>
    </NODE>

    <NODE id="tampo_esqerdo">       
        <MATERIAL id="null" />    
        <TEXTURE id="Wood" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />
        </DESCENDANTS>
    </NODE>

    <NODE id="tampo_de_baixo">       
        <MATERIAL id="null" />    
        <TEXTURE id="Wood" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="rectangulo" />
        </DESCENDANTS>
    </NODE>

    <NODE id="pernas">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <DESCENDANTS>
            <DESCENDANT id="perna1" />
            <DESCENDANT id="perna2" />
            <DESCENDANT id="perna3" />
            <DESCENDANT id="perna4" />
        </DESCENDANTS>
    </NODE>

    <NODE id="perna1">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="cilindro" />
        </DESCENDANTS>
    </NODE>

    <NODE id="perna2">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="cilindro" />
        </DESCENDANTS>
    </NODE>

    <NODE id="perna3">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="cilindro" />
        </DESCENDANTS>
    </NODE>

    <NODE id="perna4">       
        <MATERIAL id="defaultMaterial" />    
        <TEXTURE id="null" />

        <!--Translate-->

        <DESCENDANTS>
            <DESCENDANT id="cilindro" />
        </DESCENDANTS>
    </NODE>
</NODES>

</SCENE>